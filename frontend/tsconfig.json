{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "types": ["expo-router/types"],
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "types",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts",
    "types/**/*.d.ts", 
    "app/**/*"
  ]
}